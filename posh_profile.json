{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "fetch_virtual_env": true
          },
          "template": "({{ .Venv }}) ",
          "foreground": "#1fd000",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "{{ .Path }} ",
          "foreground": "#cfcfcf",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .UpstreamIcon }}{{ .HEAD }} ",
          "foreground": "#0073ff",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "> ",
          "foreground": "#cfcfcf",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3
}
