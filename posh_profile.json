{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "foreground": "#1fd000",
          "properties": {
            "fetch_virtual_env":true
          },
          "template": "({{ .Venv }}) ",
          "type": "python"
        },
        {
          "foreground": "#cfcfcf",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#0073ff",
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }} ",
          "type": "git"
        },
        {
          "foreground": "#cfcfcf",
          "style": "plain",
          "template": "> ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}